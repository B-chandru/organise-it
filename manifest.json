{
"manifest_version": 2 ,
"name":"Organise It",
"version":"1.0",
"description":"organise all your websites at one place ",
"icons":{
    "128":"images/icon128.png",
    "48":"images/icon48.png",
    "16":"images/icon16.png"
},
"browser_action":{
    "default_icon":{
        "128":"images/icon128.png",
        "48":"images/icon48.png",
        "16":"images/icon16.png"
    },
    "default_popup":"popup.html",
    "default_title":"Organise It"
},
"permissions": [
    "storage"
  ],
"content_scripts":[
    {"matches":["<all_urls>"],
    "js":["design.js"]}
]
}